{
  "generator": "readable",
  "bundle": {
    "require_mode": {
      "name": "luau"
    }
  },
  "rules": [
    "filter_after_early_return",
    "convert_index_to_field",
    "convert_local_function_to_assign",
    "convert_luau_number",
    "convert_square_root_call",
    "remove_spaces",
    "remove_comments",
    "remove_empty_do",
    "remove_nil_declaration",
    "remove_unused_if_branch",
    "remove_unused_while",
    "remove_unused_variable"
  ]
}
