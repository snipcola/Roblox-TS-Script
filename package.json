    "release": "git push origin main:release --force"
  },
  "dependencies": {
    "chokidar": "4.0.3",
    "colorette": "2.0.20",
    "lookpath": "1.2.3",
    "prompts": "2.4.2",
    "roblox-ts": "3.0.0",
    "yocto-spinner": "0.2.3"
  },
  "devDependencies": {
    "prettier": "3.5.3"
  }
}
